{
  "manifest_version": 3,
  "name": "Pro Snippet Tool",
  "version": "1.0.0",
  "description": "キーワードを定型文に置換する。",
  "permissions": ["storage"],
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ]
}
